-- 1. æœåŠ¡åˆå§‹åŒ–ï¼ˆè·å–å¿…è¦æœåŠ¡ï¼‰
local DataStoreService = game:GetService("DataStoreService")
local Players = game:GetService("Players")

-- 2. æ•°æ®å­˜å‚¨é…ç½®ï¼ˆå­˜å‚¨å¡å¯†å…‘æ¢è®°å½•ï¼Œé¿å…é‡å¤å…‘æ¢ï¼‰
local CodeDataStore = DataStoreService:GetDataStore("PlayerCodeRedeems") -- æ•°æ®å­˜å‚¨åç§°

-- 3. æœ‰æ•ˆå¡å¯†åˆ—è¡¨ï¼ˆå¯è‡ªå®šä¹‰å¡å¯†ã€å¥–åŠ±ç±»å‹å’Œæ•°é‡ï¼‰
local ValidCodes = {
    {Code = "YEET2024", RewardType = "Coins", Amount = 100},   -- å¡å¯†1ï¼š100é‡‘å¸
    {Code = "OMGVISITS", RewardType = "Coins", Amount = 200}, -- å¡å¯†2ï¼š200é‡‘å¸
    {Code = "VIP7DAY", RewardType = "VIP", Duration = 7}      -- å¡å¯†3ï¼š7å¤©VIPï¼ˆç¤ºä¾‹ï¼Œéœ€è‡ªè¡Œæ‰©å±•VIPåŠŸèƒ½ï¼‰
}

-- 4. æ ¸å¿ƒå‡½æ•°ï¼šéªŒè¯å¡å¯†æ˜¯å¦æœ‰æ•ˆ
local function isValidCode(code)
    code = string.upper(code) -- å¿½ç•¥å¤§å°å†™ï¼Œæ–¹ä¾¿ç©å®¶è¾“å…¥
    for _, validCode in ipairs(ValidCodes) do
        if validCode.Code == code then
            return validCode -- è¿”å›å¡å¯†å¯¹åº”çš„å¥–åŠ±é…ç½®
        end
    end
    return nil -- å¡å¯†æ— æ•ˆ
end

-- 5. æ ¸å¿ƒå‡½æ•°ï¼šè®°å½•å¡å¯†å…‘æ¢çŠ¶æ€ï¼ˆé¿å…é‡å¤å…‘æ¢ï¼‰
local function recordCodeRedeem(player, code)
    local playerId = "Player_" .. player.UserId
    local codeKey = playerId .. "_" .. string.upper(code) -- ç”Ÿæˆå”¯ä¸€é”®å€¼
    
    local success, hasRedeemed = pcall(function()
        -- è¯»å–å½“å‰å…‘æ¢çŠ¶æ€ï¼Œæœªè®°å½•åˆ™é»˜è®¤ä¸ºfalse
        local currentState = CodeDataStore:GetAsync(codeKey) or false
        if not currentState then
            -- æœªå…‘æ¢è¿‡ï¼Œæ ‡è®°ä¸ºå·²å…‘æ¢
            CodeDataStore:SetAsync(codeKey, true)
            return false
        end
        return true -- å·²å…‘æ¢è¿‡
    end)
    
    if not success then
        warn("æ•°æ®å­˜å‚¨å¤±è´¥ï¼š" .. tostring(hasRedeemed)) -- æ‰“å°é”™è¯¯ä¿¡æ¯
        return true -- å­˜å‚¨å¤±è´¥æ—¶ï¼Œæš‚ä¸è®©ç©å®¶å…‘æ¢
    end
    return hasRedeemed
end

-- 6. è¾…åŠ©å‡½æ•°ï¼šç»™ç©å®¶åˆ›å»ºLeaderstatsï¼ˆç”¨äºæ˜¾ç¤ºé‡‘å¸ï¼‰
local function setupLeaderstats(player)
    local leaderstats = Instance.new("Folder")
    leaderstats.Name = "Leaderstats" -- åç§°å›ºå®šï¼Œå¦åˆ™Robloxä¸æ˜¾ç¤º
    leaderstats.Parent = player
    
    local coins = Instance.new("IntValue")
    coins.Name = "Coins"
    coins.Value = 0
    coins.Parent = leaderstats
end

-- 7. è¾…åŠ©å‡½æ•°ï¼šå‘æ”¾å¥–åŠ±
local function giveReward(player, rewardConfig)
    if rewardConfig.RewardType == "Coins" then
        -- å‘æ”¾é‡‘å¸
        local coins = player.Leaderstats:FindFirstChild("Coins")
        if coins then
            coins.Value = coins.Value + rewardConfig.Amount
            player:SendMessage("å…‘æ¢æˆåŠŸï¼è·å¾—" .. rewardConfig.Amount .. "é‡‘å¸")
        end
    elseif rewardConfig.RewardType == "VIP" then
        -- VIPå¥–åŠ±ï¼ˆç¤ºä¾‹ï¼Œéœ€è‡ªè¡Œæ‰©å±•VIPæƒé™é€»è¾‘ï¼‰
        player:SendMessage("å…‘æ¢æˆåŠŸï¼è·å¾—" .. rewardConfig.Duration .. "å¤©VIP")
    end
end

-- 8. ç›‘å¬ç©å®¶åŠ å…¥ï¼Œå¤„ç†èŠå¤©è¾“å…¥çš„å¡å¯†
Players.PlayerAdded:Connect(function(player)
    -- ç©å®¶åŠ å…¥æ—¶åˆ›å»ºLeaderstats
    setupLeaderstats(player)
    
    -- ç›‘å¬ç©å®¶èŠå¤©æ¶ˆæ¯
    player.Chatted:Connect(function(message)
        -- æ£€æµ‹ç©å®¶è¾“å…¥æ ¼å¼ï¼š!code å¡å¯†ï¼ˆä¾‹å¦‚â€œ!code YEET2024â€ï¼‰
        local cmd, code = string.match(message, "^!code (%S+)$")
        if not code then
            return -- è¾“å…¥æ ¼å¼ä¸å¯¹ï¼Œå¿½ç•¥
        end
        
        -- 1. å…ˆéªŒè¯å¡å¯†æ˜¯å¦åœ¨æœ‰æ•ˆåˆ—è¡¨ä¸­
        local rewardConfig = isValidCode(code)
        if not rewardConfig then
            player:SendMessage("å¡å¯†æ— æ•ˆï¼Œè¯·æ£€æŸ¥è¾“å…¥ï¼")
            return
        end
        
        -- 2. å†æ£€æŸ¥æ˜¯å¦å·²å…‘æ¢è¿‡
        local hasRedeemed = recordCodeRedeem(player, code)
        if hasRedeemed then
            player:SendMessage("è¯¥å¡å¯†å·²è¢«å…‘æ¢ï¼Œæ— æ³•é‡å¤ä½¿ç”¨ï¼")
            return
        end
        
        -- 3. ä»¥ä¸Šéƒ½é€šè¿‡ï¼Œå‘æ”¾å¥–åŠ±
        giveReward(player, rewardConfig)
    end)
end)

